<odoo>
    <data>
        <record id="base_netimpex_url" model="ir.config_parameter">
            <field name="key">netimpex_odoo_sync.api_url</field>
            <field name="value">http://www.forward.netimpex.com/</field>
        </record>

        <record id="ir_cron_scheduler_demo_action" model="ir.cron">
            <field name="name">Netimpex Synchronization Scheduler</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="True" name="doall"/>
            <field name="model_id" ref="product.model_product_product"/>
            <field name="state">code</field>
            <field name="code">model.get_products_from_netimpex()</field>
        </record>

        <record id="ir_cron_remove_red_article" model="ir.cron">
            <field name="name">Remove redundant article</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="True" name="doall"/>
            <field name="model_id" ref="product.model_product_product"/>
            <field name="state">code</field>
            <field name="code">model.remove_redundant_article()</field>
        </record>
    </data>
</odoo>